{
  "expo": {
    "name": "holder",
    "slug": "holder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dbyilmaz.holder"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {},
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {}
  }
}
